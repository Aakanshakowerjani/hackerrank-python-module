n=int(input())
l=[]
d={}
for i in range(n):
    s=str(input())
    l.append(s.split())
search_element=str(input())
for i in l:
    for j in i:
        if search_element in j:
            indi=l.index(i)
            ls=l[indi]
            
for i in ls:
    s=float(ls[1])+float(ls[2])+float(ls[3])
print("%.2f" % (s/3))

